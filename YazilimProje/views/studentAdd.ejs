                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Adı</label>
                                        <input type="text" class="form-control" name="ad" placeholder="Öğrenci Adı">
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Soyadı</label>
                                        <input type="text" class="form-control" name="soyad" placeholder="Öğrenci Soyadı">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Okulu</label>
                                        <select class="custom-select" name="okul_ID">
                                            <% result2.forEach(result => { %>
                                              <option value="<%=result.okul_ID%>"><%=result.okul_adi%></option>
                                              <% }) %>
                                          </select>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Fakültesi</label>
                                        <select class="custom-select" name="fakulte_ID">
                                            <% result3.forEach(result => { %>
                                              <option value="<%=result.fakulte_ID%>"><%=result.fakulte_adi%></option>
                                              <% }) %>
                                          </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Bölümü</label>
                                        <select class="custom-select" name="bolum_ID">
                                            <% results.forEach(result => { %>
                                              <option value="<%=result.bolum_ID%>"><%=result.bolum%></option>
                                              <% }) %>
                                          </select>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <!-- text input -->
                                    <div class="form-group">
                                        <label>Öğrencinin Sınıfı</label>
                                        <input type="text" class="form-control" name="sınıf" placeholder="Öğrencinin Sınıfı">
                                    </div>
                                </div>
                            </div>
